{
  "name": "enzi-coffee-shop-backend",
  "version": "1.0.0",
  "description": "Backend API for Enzi Coffee Shop POS System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "build:production": "NODE_ENV=production tsc",
    "build:staging": "NODE_ENV=staging tsc",
    "build:clean": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "start:production": "NODE_ENV=production node dist/index.js",
    "start:staging": "NODE_ENV=staging node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "coffee-shop",
    "pos",
    "api",
    "express",
    "typescript"
  ],
  "author": "Enzi Coffee Shop Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.12.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "zod": "^4.0.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2"
  }
}
